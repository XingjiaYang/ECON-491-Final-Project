df_cleaned[, i] = NA
}
}
df_cleaned = df_cleaned %>%
select(where(~ !all(is.na(.x))))
df_cleaned = df_cleaned %>%
select(-CONTROL)
set.seed(123)
set_full = df_cleaned %>%
mutate(id = 1:16834)
