if (is.factor(x_var)) {
p = ggplot(set_training, aes(x = factor(.data[[var_name]]), y = INTRATE)) +
geom_boxplot() +
theme_minimal() +
labs(x = var_name, y = "Interest Rate")
} else {
p = ggplot(set_training, aes(x = .data[[var_name]], y = INTRATE)) +
geom_point(alpha = 0.6) +
theme_minimal() +
labs(x = var_name, y = "Interest Rate")
